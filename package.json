{
  "name": "bitcake",
  "private": true,
  "version": "0.2.2",
  "type": "module",
  "description": "A modern, unified web interface for Transmission and qBittorrent",
  "scripts": {
    "dev": "vite",
    "dev:transmission": "vite --mode transmission",
    "dev:qbittorrent": "vite --mode qbittorrent",
    "build": "vue-tsc -b && vite build",
    "build:transmission": "vue-tsc -b && vite build --mode transmission",
    "build:qbittorrent": "vue-tsc -b && vite build --mode qbittorrent",
    "build:both": "pnpm run build:transmission && pnpm run build:qbittorrent",
    "preview": "vite preview",
    "preview:transmission": "vite preview --outDir dist-transmission",
    "preview:qbittorrent": "vite preview --outDir dist-qbittorrent"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "axios": "^1.13.2",
    "dayjs": "^1.11.19",
    "echarts": "^6.0.0",
    "element-plus": "^2.11.8",
    "pinia": "^3.0.4",
    "vue": "^3.5.24",
    "vue-echarts": "^8.0.1",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.2.5",
    "vue-tsc": "^3.1.4"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.2.5"
    }
  }
}
